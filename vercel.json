{
	"rewrites": [
		{
			"source": "/((?!robots\\\\.txt|sitemap\\\\.xml|.*\\\\.(png|jpg|jpeg|gif|svg|ico|css|js|woff|woff2|ttf|eot|webp|mp4|mp3|pdf)).*)",
			"destination": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Strict-Transport-Security",
					"value": "max-age=31536000; includeSubDomains; preload"
				},
				{
					"key": "Cross-Origin-Opener-Policy",
					"value": "same-origin"
				}
			]
		}
	]
}
